
URxvt.scrollBar: false

#URxvt.font: xft:Source Code Pro for Powerline:size=8
/* URxvt.font: xft:MesloLGS NF:size=10 */
URxvt.font: xft:Iosevka Term:size=11
/* URxvt.font: xft:Hack Nerd Font Mono:size=10 */
URxvt*.transparent: true

!URxvt*.shading: 0 to 99 darkens, 101 to 200 lightens

URxvt*.shading: 13

#URxvt.font: xft:Anonymice Powerline:size=9
! special
*.foreground:   #ababab
*.background:   #000000
*.cursorColor:  #ababab

! black
*.color0:       #000000
*.color8:       #343434

! red
*.color1:       #e92f2f
*.color9:       #e92f2f

! green
*.color2:       #0ed839
*.color10:      #0ed839

! yellow
*.color3:       #dddd13
*.color11:      #dddd13

! blue
*.color4:       #3b48e3
*.color12:      #3b48e3

! magenta
*.color5:       #f996e2
*.color13:      #f996e2

! cyan
*.color6:       #23edda
*.color14:      #23edda

! white
*.color7:       #ababab
*.color15:      #f9f9f9

